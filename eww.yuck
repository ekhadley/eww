(defpoll time
    :interval "1s"
    :initial "{}"
    `date +'{
        "hr":"%I",
        "hour":"%-H",
        "hr":"%-I",
        "min":"%M",
        "sec":"%S",
        "s":"%s",
        "month":"%B",
        "mon":"%^b",
        "day":"%d",
        "year":"%Y"
    }'`
)